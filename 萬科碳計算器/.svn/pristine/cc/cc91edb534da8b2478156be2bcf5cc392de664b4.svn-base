/* pages/welcome/welcome.wxss */
.image {
 width: 100%;
height: 1030rpx;
margin: 0rpx 20rpx 0rpx 0rpx;
float: left;

}
page {
  background: black;
}

.title {
width: 100%;
height: 27%;
display: flex;
flex-wrap: wrap;
}

.img {
  width: 100vh;
  height: 450rpx;
  /* background-color: greenyellow; */
}

.tab1 {
 width: 35%;
height: 170rpx;
font-size: 26rpx;
font-weight: 556;
color: white;
margin:0rpx auto;
}

.put {
width: 95rpx;
height: 95rpx;
border-radius: 50%;
background: none;
margin:213rpx auto;
background-repeat: no-repeat;
background-size: cover;
}
.content:nth-of-type(2)>.put{
background-image:url("http://wkjsq.carbonstop.net/static/icon/next.png");
transform: rotate(180deg);

}

.content:nth-of-type(3) >.put{
background-image:url("http://wkjsq.carbonstop.net/static/icon/next.png");
margin-top: 213rpx;



}
.main {
  width: 100%;
  height: 100vh;
  /* background: linear-gradient(to bottom left,#78DFC4, #69D6ED ); */
  background-position: 68% 100%;
  background-repeat: no-repeat;
  background-size: 100% 100;
  /* padding-top: 180rpx; */
}

.circle {
  width: 319rpx;
  height: 414rpx;
  background-image: url(http://wkjsq.carbonstop.net/static/bj/6.png);
  position: relative;
  bottom: -10%;
  left: 31%;
  background-position: 11% 10%;
  background-size: contain;
  background-repeat: no-repeat;
}

.text {
  text-align: center;
  position: relative;
  top: 10%;
  font-size: 24rpx;
  color: #202121;
  margin-bottom: 15rpx;
}

.content {
  width: 50%;
height: 50%;
float: left;

}

.content:nth-of-type(2) {
  background-image: url("http://wkjsq.carbonstop.net/static/bj/community.png");
background-size: cover;

}

.content:nth-of-type(3) { 
  background-image: url("http://wkjsq.carbonstop.net/static/bj/meeting.png");
  background-size: cover;
}

.footer {
  width: 100%;
  height: 14%;
  /* background: green; */
  position: absolute;
  bottom: 0%;
}
.titleImg{
display: flex;
width: 100%;
justify-content: space-around;
height: 107rpx;
margin-top: 138rpx;
}
.title_img{
 width: 12%;
height: 94rpx;
/* background: red; */
}
.title_img1{
background-image:url("http://wkjsq.carbonstop.net/static/icon/vanke.png");
background-size: contain;
background-repeat: no-repeat;
}
.title_img2{
  background-image:url("http://wkjsq.carbonstop.net/static/icon/cslogo.png");
  background-size: contain;
  background-repeat:no-repeat;
  background-position: 100%;
}
.title_img3{
background-image: url("http://wkjsq.carbonstop.net/static/icon/history.png");
background-repeat: no-repeat;
background-position: 100% 100%;
background-size: cover;

}
.title_text{
  color: white;
font-size: 51rpx;
font-weight: 700;
text-align: center;
margin: 0rpx auto;

}
.help{
width: 93rpx;
height: 93rpx;
border-radius: 50%;
position: absolute;
right: 11%;
top: 25%;
background-image: url("http://wkjsq.carbonstop.net/static/icon/help.png");
background-size: contain;
background-repeat: no-repeat;

}
