/* pages/class/class.wxss */
.image {
 width: 100%;
height: 1030rpx;
margin: 0rpx 20rpx 0rpx 0rpx;
float: left;

}
page {
height: 100vh;
background:#144B9E;
overflow: hidden;
color: white;
}
.but {
width: 90rpx;
height: 90rpx;
border-radius: 50%;
overflow: hidden;
margin-top: 26rpx;

}
.title_img {
width: 40rpx;
height: 40rpx;
transform: scale(2.1);
display: block;
margin-top: 27rpx;
margin-left: -3rpx;
}
.title {
 height: 61%;
display: flex;
flex-wrap: wrap;
justify-content: space-around;
width: 100rpx;
}

.main {
width: 100%;
float: left;
display: flex;
height: 22%;
background: #144B9E;
justify-content: center;
overflow: hidden;
position: absolute;
bottom: 0px;
z-index: 99;
}
.content_img{
  width: 47rpx;
height: 100%;
/* background: yellow; */
}
/* .active{
 background: rgba(255,255,255,1);
} */
.active::before{
content: "";
width: 292rpx;
height: 100rpx;
background: rgba(110,113,113,0.15);
display: block;
position: absolute;
box-shadow: 0rpx 0rpx 30rpx 0rpx #91DAD3;
}
.content {
  display: flex;
  align-items: center;
  height: 89rpx;
}

.content_img {
  width: 47rpx;
  height: 45rpx;
  /* background: yellow; */
  display: block;
  float: right;
  margin-right: 10rpx;

}
.mainRight {
  width: 76%;
  height: 100vh;
  /* background: blue; */
}
.meetTitle{
  width: 85%;
margin: 0rpx auto;
}
.Right {
  width: 100%;
  height: 100%;
}

/* 点击下一步按钮 */

.next {
width: 173rpx;
height: 75rpx;
font-size: 33rpx;
background: #F6CB00;
border: none;
color: white;
text-align: center;
line-height: 75rpx;
position: absolute;
left: 38%;
bottom: 12%;
}

.input {
  width: 100rpx;
  height: 22rpx;
  text-align: center;
  border-bottom: 1rpx solid gray;
}

.content {
  display: flex;
  font-size: 26rpx;
  justify-content: space-around;
}

.name {
display: flex;
width: 221rpx;
height: 45rpx;
font-size: 33rpx;
/* background: red; */
}

.unit {
  width: 73rpx;
  height: 27rpx;
  display: block;
  text-align: right;
  font-size: 33rpx;
}

.active {
}

.but::after {
  border: none;
}

/* 人数 */

.numn {
  position: absolute;
  top: 15.5%;
  right: 0%;
}

button>text {
  text-align: center;
  width: 100%;
  display: block;
}

/*回退按钮 */

.back {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: red;
  position: absolute;
  left: 10%;
  top: 10%;
}
